
{
    "manifest_version": 3,
    "name": "Linkify",
    "version": "1.0.0",
    "description": "Organize and manage your favorite links",
    "permissions": ["storage"],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
